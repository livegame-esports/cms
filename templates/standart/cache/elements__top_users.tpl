
<!-- Start elements/top_users.tpl -->
<a href="../profile?id={id}">
	<img src="../{avatar}" alt="{login}">
	<div>
		<span style="color: {gp_color}" title="{gp_name}" tooltip="yes">{login}</span>
		<p>
			<span title="Рейтинг" tooltip="yes"><i class="fas fa-star"></i> {reit}</span><span title="Сообщений" tooltip="yes"><i class="fas fa-envelope"></i> {answers}</span><span title="Спасибок" tooltip="yes"><i class="fas fa-hand-point-right"></i> {thanks}</span>
		</p>
	</div>
</a>
<!-- End elements/top_users.tpl -->
